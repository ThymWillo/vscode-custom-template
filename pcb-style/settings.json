{
    "workbench.colorCustomizations": {
        "activityBar.background": "#0a5e42",  // Couleur de fond de la barre latérale
        "activityBar.foreground": "#ffffffc5",  // Couleur du texte et des icônes
        "activityBar.inactiveForeground": "#339253",  // Couleur du texte et des icônes inactives
        "activityBarBadge.background": "#313634",  // Couleur de fond de l'insigne de la barre latérale
        "activityBarBadge.foreground": "#929c98",  // Couleur du texte de l'insigne de la barre latérale

        "statusBar.background": "#0a5e42",  // Couleur de fond de la barre d'état
        "statusBar.foreground": "#ffffffc5",  // Couleur du texte de la barre d'état
